{
  "name": "docpal-node-6",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:generate": "nuxt db generate",
    "db:migrate": "nuxt db migrate",
    "db:drop": "nuxt db drop",
    "db:sql": "nuxt db sql",
    "docker:up": "docker compose -f docker-compose.dev.yml up -d && sleep 3 && pnpm postgis:enable",
    "docker:down": "docker compose -f docker-compose.dev.yml down",
    "docker:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "docker:restart": "pnpm docker:down && pnpm docker:up",
    "postgis:enable": "bash scripts/enable-postgis.sh",
    "db:seed-company": "docker exec -i docpal-postgres psql -U docpal -d docpal < scripts/seed-dummy-company.sql",
    "db:reset": "bash scripts/reset-db.sh",
    "setup": "pnpm docker:up && pnpm db:generate && pnpm db:migrate && pnpm db:seed-company",
    "seed": "bash scripts/seed.sh",
    "test:middleware": "bash test-middleware-context.sh",
    "type-check": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "@nuxt/icon": "2.1.1",
    "@nuxthub/core": "^0.10.3",
    "@tiptap/pm": "^3.14.0",
    "@tiptap/starter-kit": "^3.14.0",
    "@tiptap/vue-3": "^3.14.0",
    "@types/bcrypt": "^6.0.0",
    "@types/leaflet": "^1.9.21",
    "bcrypt": "^6.0.0",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "element-plus": "^2.12.0",
    "leaflet": "^1.9.4",
    "nanoid": "^5.1.6",
    "nuxt": "^4.2.2",
    "openai": "^6.15.0",
    "postgres": "^3.4.7",
    "typescript": "^5.9.3",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4",
    "vuedraggable": "^4.1.0",
    "vxe-pc-ui": "~4.11.12",
    "vxe-table": "~4.17.32",
    "xe-utils": "~3.8.0"
  },
  "packageManager": "pnpm@8.5.1+sha1.97019f5a8ec2416123506419ab36dd558e4c72eb",
  "devDependencies": {
    "@element-plus/nuxt": "^1.1.4",
    "@iconify-json/lucide": "^1.2.81",
    "@iconify-json/tabler": "^1.2.24",
    "sass": "^1.97.0"
  }
}
