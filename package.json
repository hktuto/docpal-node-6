    "drizzle-orm": "^0.45.1",
    "element-plus": "^2.12.0",
    "nuxt": "^4.2.2",
    "postgres": "^3.4.7",
    "typescript": "^5.9.3",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4",
    "vuedraggable": "^4.1.0"
  },
  "packageManager": "pnpm@8.5.1+sha1.97019f5a8ec2416123506419ab36dd558e4c72eb",
