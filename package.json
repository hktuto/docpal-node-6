    "setup": "pnpm docker:up && sleep 3 && pnpm db:generate && pnpm db:migrate",
    "seed": "bash scripts/seed.sh",
    "type-check": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
